c/scene/container/utils/checkChildrenDidChange.ts","name":null,"original":{"line":46,"column":35},"generated":{"line":18,"column":34}},{"source":"../../../../src/scene/container/utils/checkChildrenDidChange.ts","name":null,"original":{"line":46,"column":42},"generated":{"line":18,"column":36}},{"source":"../../../../src/scene/container/utils/checkChildrenDidChange.ts","name":null,"original":{"line":46,"column":54},"generated":{"line":18,"column":48}},{"source":"../../../../src/scene/container/utils/checkChildrenDidChange.ts","name":null,"original":{"line":46,"column":54},"generated":{"line":18,"column":49}},{"source":"../../../../src/scene/container/utils/checkChildrenDidChange.ts","name":null,"original":{"line":46,"column":54},"generated":{"line":19,"column":4}},{"source":"../../../../src/scene/container/utils/checkChildrenDidChange.ts","name":null,"original":{"line":47,"column":8},"generated":{"line":20,"column":2}},{"source":"../../../../src/scene/container/utils/checkChildrenDidChange.ts","name":null,"original":{"line":50,"column":4},"generated":{"line":21,"column":2}},{"source":"../../../../src/scene/container/utils/checkChildrenDidChange.ts","name":null,"original":{"line":50,"column":11},"generated":{"line":21,"column":9}},{"source":"../../../../src/scene/container/utils/checkChildrenDidChange.ts","name":null,"original":{"line":50,"column":24},"generated":{"line":21,"column":21}},{"source":"../../../../src/scene/container/utils/checkChildrenDidChange.ts","name":null,"original":{"line":50,"column":24},"generated":{"line":21,"column":22}},{"source":"../../../../src/scene/container/utils/checkChildrenDidChange.ts","name":null,"original":{"line":50,"column":24},"generated":{"line":21,"column":31}},{"source":"../../../../src/scene/container/utils/checkChildrenDidChange.ts","name":null,"original":{"line":51,"column":0},"generated":{"line":22,"column":0}}],"sources":{"../../../../src/scene/container/utils/checkChildrenDidChange.ts":"import type { Container } from '../Container';\n\n/**\n * This function will crawl through the container essentially check if the children have changed.\n *\n * This function checkChildrenDidChange recursively checks if any child in a Container\n * or its children has changed. It does this by comparing a generated changeId for each\n * child against a stored value in previousData.\n * The changeId is a combination of the child's uid and _didChangeId, bitwise manipulated for uniqueness.\n * If a change is detected, it updates previousData and sets didChange to true.\n * The function returns a boolean indicating if any change was detected in the entire hierarchy of children.\n * @param container - the container to check for changes\n * @param previousData - the previous data from the last check made\n * @param previousData.data - the data array\n * @param previousData.index - the index of the data array\n * @param previousData.didChange - did the data change\n */\nexport function checkChildrenDidChange(\n    container: Container,\n    previousData: {\n        data: number[];\n        index: number;\n        didChange: boolean;\n    })\n{\n    const children = container.children;\n\n    for (let i = 0; i < children.length; i++)\n    {\n        const child = children[i];\n\n        const changeId = ((child.uid & 255) << 24)\n            | (child._didChangeId & 16777215);\n\n        if (previousData.data[previousData.index] !== changeId)\n        {\n            previousData.data[previousData.index] = changeId;\n\n            previousData.didChange = true;\n        }\n\n        previousData.index++;\n\n        if (child.children.length)\n        {\n            checkChildrenDidChange(child, previousData);\n        }\n    }\n\n    return previousData.didChange;\n}\n"},"lineCount":null}},"error":null,"hash":"842d31e134f86e88fe1db1ae95daaf40","cacheData":{"env":{}}}